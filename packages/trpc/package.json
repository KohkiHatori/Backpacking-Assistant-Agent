{
  "name": "@repo/trpc",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts",
      "require": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "require": "./src/server.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["src/index.ts"],
      "client": ["src/client.ts"],
      "server": ["src/server.ts"]
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@trpc/client": "^11.0.0-rc.464",
    "@trpc/server": "^11.0.0-rc.464",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "typescript": "^5.5.3"
  }
}
