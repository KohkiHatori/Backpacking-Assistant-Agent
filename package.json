{
  "name": "Backpacking-Assistant-Agent",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "^5.5.3",
    "eslint": "^8.57.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0"
  },
  "dependencies": {
    "next": "^14.2.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
